#!/bin/sh

#Schema to instance tool

if [ -z $XMLBEANS_LIB ]; then . `dirname $0`/_setlib; fi

cp=$XMLBEANS_LIB/xbean.jar:$XMLBEANS_LIB/jsr173_api.jar

case "`uname`" in
    CYGWIN*)
        cp=`cygpath -w -p $cp`
        ;;
esac

java -classpath $cp org.apache.xmlbeans.impl.xsd2inst.SchemaInstanceGenerator $*
